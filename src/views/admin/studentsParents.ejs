    <title>TSMART</title>
  </head>
  <body>
    <div class="container row" id="container">
      <div class="boxInfor col-lg-3 col-md-12">
        <form>
          <div class="header">
            <i class="fa-solid fa-layer-group"></i>
            <span id="IdStudent" data-id="<%= idStudents %>"
              >Thông tin học sinh</span
            >
          </div>
          <div class="form-group">
            <label class="col-form-label">ID học sinh</label>
            <input
              type="text"
              value="<%=students._id%>"
              readonly="readonly"
              class="form-control"
            />
          </div>
          <div class="form-group">
            <label class="col-form-label">Mã Lớp</label>
            <input
              type="text"
              value="<%=ClassInStudesnt[0]?.nameClass || `Học sinh tạm thời chưa được xếp lớp`%>"
              readonly="readonly"
              class="form-control"
            />
          </div>
          <div class="form-group">
            <label class="col-form-label">Tên học sinh</label>
            <input
              type="text"
              value="<%=students.fullname%>"
              readonly="readonly"
              class="form-control"
            />
          </div>
          <div class="form-group">
            <label class="col-form-label">Ca học</label>
            <input
              type="text"
              value="<%=ClassInStudesnt[0]?.study || `Học sinh tạm thời chưa được xếp lớp`%>"
              readonly="readonly"
              class="form-control"
            />
          </div>
          <div class="form-group">
            <label class="col-form-label">Ngày học trong tuần</label>
            <input
              type="text"
              value="<%=ClassInStudesnt[0]?.days || `Học sinh tạm thời chưa được xếp lớp`%>"
              readonly="readonly"
              class="form-control"
            />
          </div>
        </form>
      </div>
      <div class="feedBackBox col-lg-9 col-md-12">
        <div class="row">
          <h3 class="col-9">Danh sách Feedback từ giáo viên</h3>
          <div class="item col-3">
            <label for="time" class="disabled fw-medium">Thời gian</label>
